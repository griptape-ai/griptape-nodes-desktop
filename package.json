  "version": "0.1.3",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "pack:osx": "./scripts/build-osx.sh",
    "pack:linux": "./scripts/build-linux.sh",
    "pack:windows": "powershell -ExecutionPolicy Bypass -File ./scripts/build-windows.ps1",
    "pack:windows:arm64": "powershell -ExecutionPolicy Bypass -File ./scripts/build-windows.ps1 -Architecture arm64",
    "publish:osx": "./scripts/publish-osx.sh",
    "publish:linux": "./scripts/publish-linux.sh",
    "publish:windows": "powershell -ExecutionPolicy Bypass -File ./scripts/publish-windows.ps1",
    "publish:windows:arm64": "powershell -ExecutionPolicy Bypass -File ./scripts/publish-windows.ps1 -Architecture arm64",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "generate-icons": "node scripts/generate-icons.js"
  },
  "keywords": [],
